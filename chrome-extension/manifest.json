{
    "name": "Calime",
    "version": "1.0",
    "description": "A calender for your anime schedule",
    "manifest_version": 2,
    "key": "",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab",
        "downloads",
        "identity"
    ],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    },
    "web_accessible_resources": [
        "/images/miku.jpg"
    ]
}